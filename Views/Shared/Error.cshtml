@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>

    <meta name="description" content="Hypster.TV includes Movies, Music, Radio, TV and Online entertainment stories and reporting from Hollywood and around the globe." />
    <meta name="keywords" content="Hypster.TV, Music, Radio, TV and the Net, reporting, bands, tributes, groups, rock, headliners, rap, movies, classical, 80\'s, jams, actors, a-listers, the business, hollywood, gigs, big bands, swing, jazz, foreign, irish, standards, glamour" />




    <script src="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css"  type="text/javascript"></script>
    <script src="http://code.jquery.com/jquery-1.8.3.min.js"  type="text/javascript"></script>
    <script src="http://code.jquery.com/ui/1.10.1/jquery-ui.js"  type="text/javascript"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>


    @Scripts.Render("~/scripts/hypScript.js?id=9231872")
    @Scripts.Render("~/scripts/hypSlide.js?id=182763")
    
    <link href="/css/Site.css" rel="stylesheet" type="text/css" />



</head>





<body>
    <div style=" float:left; width:100%; min-width:1040px; min-height: 100%; height: auto !important; height: 100%; margin-bottom:25px; ">
        

        <div class="ContentHeaderCont">
           
                <div class="ContentHeaderInner">
                    <!-- MAIN MENU -->
                    <div id="MainMenu" >
                        <img class="mLogo" alt="Hypster Logo" src="/imgs/shared/main_logo.png" onclick="window.location='/';" />


                        <div class="hypContHolder" onmouseover="$('#HypsterSub').css('display','block');" onmouseout="$('#HypsterSub').css('display','none');">
                            <a class="mHypster" href="/" >
                                Hypster
                            </a>
                            <!-- SUB MENU -->
                            <div id="HypsterSub">
                                <a class="HypSub_tv" href="http://@System.Configuration.ConfigurationManager.AppSettings["videoHostName"]">TV</a>
                                <a class="HypSub_news" href="http://@System.Configuration.ConfigurationManager.AppSettings["newsHostName"]">News</a>
                            </div>
                            <!-- SUB MENU -->
                        </div>


                        <a class="mListen" href="/listen">Listen</a>
                        <a class="mCreate" href="/create">Create</a>
                        <a class="mConnect" href="/connect">Connect</a>

                

                        <div class="TopHeaderCont">
                        </div>
                    </div>
                    <!-- MAIN MENU -->
                </div>

            
        </div>





        <div class="ContentRootCont" >

            
            <script type="text/javascript">
                $(document).ready(function () {
                    RenderError();
                });
                $(window).resize(function () {
                    RenderError();
                });
            </script>
             

             <div class="boxContRight">
                <div id="listenSlide1" class="slideRight">
                    
                    <div style=" float:left; width:100%; height:400px; font-size:22px;">
                        Error occurred!!! Our team is working to resolve this problem.
                    </div>

                </div>
            </div>

             
        </div>
        

    
        <div class="push"></div>
    </div>





    <!-- BOTTOM -->
    <div class="ContentFooterCont">
        <center>

                <div class="BottomPane">
                    <img alt="" src="/imgs/shared/footer_logo.png" />

                    <div class="MenuItmCont">
                        <a href="/" >Hypster</a>
                        <a href="/listen" >Listen</a>
                        <a href="/create" >Create</a>
                        <a href="/connect" >Connect</a>
                    </div>

                    <div class="MenuItmCont">
                        <a href="/listen/radio" >Radio</a>
                        <a href="http://@System.Configuration.ConfigurationManager.AppSettings["videoHostName"]" >TV</a>
                        <a href="http://@System.Configuration.ConfigurationManager.AppSettings["newsHostName"]" >Music News</a>
                    </div>
                        
                    <div class="MenuItmCont">
                        <a href="/hypster/ContactUs" >Contact Us</a>
                        <a href="/hypster/AboutUs" >About Us</a>
                        <a href="/hypster/PrivacyPolicy" >Privacy Policy</a>
                        <a href="/hypster/TermsofService" >Terms of Service</a>
                    </div>
                </div>

        </center>
    </div>
    <!-- BOTTOM -->






    <!-- hide back -->
    <div class="Trans_BACK" id="HideBackDiv" onclick="Show_Content();"></div>
    <div id="popupContainer"></div>
    <!-- hide back -->






    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-28695705-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>


    
   
</body>
</html>